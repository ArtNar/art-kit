{
  "name": "artn-kit",
  "version": "1.0.18",
  "description": "ui-kit",
  "main": "main.js",
  "scripts": {
    "start": "styleguidist server",
    "clear": "rm -rf dist",
    "build:demo": "styleguidist build",
    "build": "npm run clear && babel src -d dist && gulp sass",
    "release": "npm run build && cp package.json ./dist/",
    "release-publish": "npm publish ./dist",
    "release-patch": "npm version patch -m 'chore(*): patch version'",
    "release-minor": "npm version minor -m 'chore(*): minor version'",
    "release-major": "npm version major -m 'chore(*): major version'"
  },
  "keywords": [
    "ui-kit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ArtNar/art-kit.git"
  },
  "homepage": "https://github.com/ArtNar/art-kit#readme",
  "author": "Artem Narozhniy",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "file-loader": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-sass": "^4.0.2",
    "node-sass": "^4.13.1",
    "react": "^16.13.0",
    "react-docgen": "^5.3.0",
    "react-dom": "^16.13.0",
    "react-styleguidist": "^11.0.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
