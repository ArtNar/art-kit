{
    "presets": [
        "@babel/preset-react",
        [
            "@babel/preset-env",
            {
                "corejs": 3,
                "modules": false,
                "shippedProposals": true,
                "targets": {
                    "browsers": "defaults"
                },
                "useBuiltIns": "usage"
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-dynamic-import"
    ]
}
